// Set the skeleton that will be used for this animator
skel 			Model.fbx


// Add an animation with correct enum (case sensitive)
anim 			IDLE 			Model_Idle.fbx


// Possible to set options for animation. 
// Set by default if nothing was set.

// Can be repeated when set to 1.
loopable		IDLE			0

// How fast an animation should go.
tickSpeed		IDLE			30

// From this bone and up we will use upper animation
upperbodyBone	QuickRigCharacter_Spine


// Animation state between two animations
// 				from			to					blendtime (seconds)
state			IDLE 	 		RUN					1.0
state			IDLE 	 		PRIMARY_ATTACK		1.0
